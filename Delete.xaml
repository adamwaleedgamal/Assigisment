<Page x:Class="Registeration___Login_Desktop_Application.Delete"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Registeration___Login_Desktop_Application"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Delete">

    <Grid Background="White">
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="41*"/>
                <RowDefinition Height="49*"/>
            </Grid.RowDefinitions>
            <Label Content="Phone Number :" HorizontalAlignment="Left" Margin="82,53,0,0" VerticalAlignment="Top" FontSize="24"/>
            <Button Style="{StaticResource Button}" x:Name="Delete_but" Content="Delete" HorizontalAlignment="Left" Margin="605,65,0,0" VerticalAlignment="Top" Width="135" Click="Delete_but_Click" Height="25"/>
            <TextBox x:Name="Ph_txt" Style="{StaticResource text_box}" HorizontalAlignment="Center" Margin="0,65,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="230"/>
        </Grid>

    </Grid>
</Page>
